<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/es_home_bg"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:isScrollContainer="true"
    android:scrollbars="vertical" >
   
    <ImageButton 
        android:id="@+id/foodBtn"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="164dp"
        android:layout_marginLeft="74dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/es_shop_btn_food"
        />

    <ImageButton
        android:id="@+id/beautyBtn"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_above="@+id/foodBtn"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="21dp"
        android:layout_marginRight="32dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/es_shop_btn_beauty" />

    <ImageButton
        android:id="@+id/cafeBtn"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignBottom="@+id/beautyBtn"
        android:layout_alignLeft="@+id/foodBtn"
        android:layout_marginBottom="56dp"
        android:layout_marginLeft="26dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/es_shop_btn_cafe" />

    <ImageButton
        android:id="@+id/entertainBtn"
        android:layout_width="80dip"
        android:layout_height="80dip"
         android:layout_alignBottom="@+id/othersBtn"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="42dp"
        android:layout_marginRight="5dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/es_shop_btn_entertain" />

    <ImageButton
        android:id="@+id/othersBtn"
        android:layout_width="81dip"
        android:layout_height="88dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="54dp"
        android:layout_marginLeft="73dp"
        android:background="#00FFFFFF"
        android:scaleType="fitXY"
        android:src="@drawable/es_shop_btn_extra" />

</RelativeLayout>